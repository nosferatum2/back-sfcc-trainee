{
  "name": "LYONSCG-MFRA",
  "version": "0.0.22",
  "description": "LYONSCG Mobile First Reference Application",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "node build_tools/build --test ../test/unit/**/*.js",
    "test:appium": "node bin/Makefile functional -- appium -- ",
    "cover": "node build_tools/build --cover",
    "test:integration": "node bin/Makefile integration -- ",
    "test:functional": "node bin/Makefile functional -- ",
    "test:functional:docker": "bin/test-functional-docker.sh",
    "test:functional:sauce": "./node_modules/.bin/wdio test/functional/webdriver/wdio.conf.js ",
    "compile:scss": "node build_tools/build --compile css",
    "compile:js": "node build_tools/build --compile js",
    "compile:fonts": "node bin/Makefile compileFonts",
    "lint": "node build_tools/build --lint css && node build_tools/build --lint js",
    "upload": "node build_tools/build --upload",
    "uploadCartridge": "node build_tools/build --upload-cartridge --cartridge app_storefront_base && node build_tools/build --upload-cartridge --cartridge modules",
    "watch": "node build_tools/build --watch",
    "release": "node bin/Makefile release --"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:lyonsconsultinggroup/reference-application-mobile-first.git"
  },
  "author": "Ilya Volodin <ivolodin@demandware.com>",
  "contributors": [
    "Ryan Walter <rwalter@lyonscg.com>"
  ],
  "homepage": "https://bitbucket.org/lyonsconsultinggroup/reference-application-mobile-first#readme",
  "devDependencies": {
    "@tridnguyen/config": "^2.3.1",
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-subset": "^1.5.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "cldr-data": "^29.0.1",
    "dwupload": "^3.8.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "globalize": "^1.1.1",
    "inquirer": "^3.0.6",
    "istanbul": "^0.4.4",
    "lodash": "^4.14.2",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.12.0",
    "moment-timezone": "^0.5.5",
    "node-sass": "^4.5.0",
    "onchange": "^3.2.1",
    "optionator": "^0.8.2",
    "properties-parser": "^0.3.0",
    "proxyquire": "1.7.4",
    "request-promise": "^4.1.1",
    "selenium-standalone": "^5.9.0",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.4",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-scss": "^1.3.4",
    "url": "^0.11.0",
    "wdio-sauce-service": "~0.1",
    "webdriverio": "^3.3.0",
    "webpack": "^3.5.5",
    "xml2js": "^0.4.17"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "flag-icon-css": "^2.3.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "slick-carousel": "^1.7.1",
    "svg4everybody": "^2.1.9"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "paths": {
    "base": "../cartridges/app_storefront_base",
    "lyonscg": "../cartridges/app_lyonscg_mfra",
    "org": "../cartridges/org_organizationid_mfra",
    "site": "../cartridges/site_siteid_mfra"
  },
  "packageName": "app_storefront_base"
}
