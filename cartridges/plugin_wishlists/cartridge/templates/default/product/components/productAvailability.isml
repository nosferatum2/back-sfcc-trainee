<!-- Availability -->
<isset name="isWishlistEnabled" value="${dw.system.Site.getCurrent().getCustomPreferenceValue('enableWishlist')}" scope="page"/>
<div class="availability col-md-10 col-12 product-availability" data-ready-to-order="${product.readyToOrder}" data-available="${product.available}">
    <isinclude template="product/components/availability" />
    <isif condition="${isWishlistEnabled}">
        <isinclude template="wishlist/components/addToWishListButton" />
    </isif>
</div>
